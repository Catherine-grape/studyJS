<view class="container">
    <swiper interval="2000" indicator-dots indicator-color="#FF3030" indicator-active-color="#FF7F00" autoplay circular>
        <block wx:for="{{swiperData}}" wx:key="id">
            <swiper-item>
                <image src="{{item.imgUrl}}" class="image" mode="widthFix"></image>
            </swiper-item>
        </block>

    </swiper>
</view>