   this.$confirm(\"您确定要退出吗？\", \"温馨提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(function () {\n        // console.log(\"确定\");\n        return signOut();\n      }).then(function (result) {\n        var code = result.code;\n\n        if (code == 0) {\n          _this.$alert(\"恭喜您，退出成功\", {\n            callback: function callback() {\n              location.href = \"/login.html\";\n            }\n          });\n        } else {\n          _this.$message.error(\"不好意思退出失败请稍后重试\");\n        }\n      }).catch(function () {// console.log(\"取消\");\n      });\n    },\n    handleClose: function handleClose() {},\n    handlePass: function handlePass() {\n      var _this2 = this;\n\n      if (this.password !== this.secondPassword) {\n        this.$message.error(\"您两次输入的密码不一致，请检查后重新输入\");\n        this.password = \"\";\n        this.secondPassword = \"\";\n        return;\n      } else {\n        updataPassword({\n          password: md5(this.password)\n        }).then(function (result) {\n          var code = result.code;\n\n          if (code == 0) {\n            _this2.$message.success(\"密码修改成功\");\n          } else {\n            _this2.$message.error(\"密码修改失败，请稍后重试\");\n          }\n\n          _this2.dialogVisible = false;\n          _this2.password = \"\";\n          _this2.secondPassword = \"\";\n        });\n      }\n    }\n  },\n  created: function created() {\n    var _this3 = this;\n\n    getInfo().then(function (result) {\n      var code = result.code,\n          data = result.data;\n\n      if (code == 0) {\n        _this3.name = data.name;\n      } else {\n        _this3.$message.error(\"请求个人信息失败，请刷新重试\");\n      }\n    });\n  },\n  mounted: function mounted() {\n    var winH = document.documentElement.clientHeight,\n        headerBox = this.$refs.headerBox,\n        mainBox = this.$refs.mainBox;\n    mainBox.style.height = winH - headerBox.offsetHeight + \"px\";\n    this.$forceUpdate();